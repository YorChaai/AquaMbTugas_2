<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/backgroundImageView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="fitXY"
            android:background="@drawable/group_14"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/group_385"
                    android:layout_width="220dp"
                    android:layout_height="31dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="11dp"
                    android:layout_marginTop="12dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <Button
                        android:id="@+id/vector"
                        android:layout_width="25dp"
                        android:layout_height="20dp"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="0dp"
                        android:background="@drawable/panah1"
                        android:onClick="main_menu"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                        android:id="@+id/scan_tutup_"
                        android:layout_width="198dp"
                        android:layout_height="match_parent"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="0dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:lineSpacingExtra="3sp"
                        android:text="@string/redeemvagan"
                        android:textAppearance="@style/redeemvagan" />

                </RelativeLayout>



            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:id="@+id/rectangle_11"
                android:layout_width="347dp"
                android:layout_height="70dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="789dp"
                android:background="@drawable/rectangle_11"
                android:onClick="redeemv2"
                android:text="Redeem"
                android:textColor="@color/white" />


            <ImageView
                android:id="@+id/pls_mns_image"
                android:layout_width="54dp"
                android:layout_height="18dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="603dp"
                android:layout_marginEnd="25dp"
                android:background="@drawable/pls_mns"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/some_id"
                android:layout_width="82dp"
                android:layout_height="50dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="625dp"
                android:layout_marginEnd="9dp"
                android:lineSpacingExtra="12sp"
                android:text="@string/some1_id"
                android:textAlignment="center"
                android:textAppearance="@style/some1_id" />

        </RelativeLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
